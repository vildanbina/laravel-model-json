{
  "name": "vildanbina/laravel-model-json",
  "description": "Laravel Model JSON",
  "keywords": [
    "laravel",
    "model-export",
    "json-export"
  ],
  "homepage": "https://github.com/vildanbina/laravel-model-json",
  "license": "MIT",
  "authors": [
    {
      "name": "Vildan Bina",
      "email": "vildanbina@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "spatie/laravel-package-tools": "^1.19"
  },
  "require-dev": {
    "illuminate/database": "^11.0 || ^12.0",
    "illuminate/console": "^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "Vildanbina\\ModelJson\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vildanbina\\ModelJson\\ModelJsonServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
